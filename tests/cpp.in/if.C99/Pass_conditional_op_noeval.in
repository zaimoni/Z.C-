SUFFIXES h hpp
// code coverage: ? : prevention of evaluation errors
// (C)2009 Kenneth Boyd, license: MIT.txt

#include <stdint.h>

// relocate some valid-true and valid-false tests from pass_conditional_op.h/hpp to here
FALSE_EXPRESSION 0 ? INTMAX_MAX*INTMAX_MAX : 0
TRUE_EXPRESSION 0 ? INTMAX_MAX*INTMAX_MAX : 1
FALSE_EXPRESSION 1 ? 0 : INTMAX_MAX*INTMAX_MAX
TRUE_EXPRESSION 1 ? 1 : INTMAX_MAX*INTMAX_MAX

FALSE_EXPRESSION 0 ? 1/0 : 0
TRUE_EXPRESSION 0 ? 1/0 : 1
FALSE_EXPRESSION 1 ? 0 : 1/0
TRUE_EXPRESSION 1 ? 1 : 1/0

FALSE_EXPRESSION 0 ? 1%0 : 0
TRUE_EXPRESSION 0 ? 1%0 : 1
FALSE_EXPRESSION 1 ? 0 : 1%0
TRUE_EXPRESSION 1 ? 1 : 1%0

FALSE_EXPRESSION 0 ? INTMAX_MAX+INTMAX_MAX : 0
TRUE_EXPRESSION 0 ? INTMAX_MAX+INTMAX_MAX : 1
FALSE_EXPRESSION 1 ? 0 : INTMAX_MAX+INTMAX_MAX
TRUE_EXPRESSION 1 ? 1 : INTMAX_MAX+INTMAX_MAX

FALSE_EXPRESSION 0 ? -INTMAX_MAX-INTMAX_MAX : 0
TRUE_EXPRESSION 0 ? -INTMAX_MAX-INTMAX_MAX : 1
FALSE_EXPRESSION 1 ? 0 : -INTMAX_MAX-INTMAX_MAX
TRUE_EXPRESSION 1 ? 1 : -INTMAX_MAX-INTMAX_MAX

